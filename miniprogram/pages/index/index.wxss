button::after{
  border:none;
}
.contact_info {
  box-sizing: border-box;
  border: 1px solid #f90;
  box-shadow: 0 0 5px #f90;
  margin: 15px; 
  background-color: #f90;
  padding: 16px 0;
  border-radius: 14px;
}
.contact_info .item{
  color: #fff;
  padding: 10px;
}
.contact_info .linear{
  width: 96%;
  margin: 0 2%;
  height: 2px;
  background: linear-gradient(to right, #ff8181 0%, #ffff81 20%, #81ff81 40%, #81ffff 60%, #8181ff 80%, #ff81ff 100%); 
}
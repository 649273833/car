/* miniprogram/pages/index/index.wxss */
.clear:after{
  display: block;
  content: '';
  clear: both;
}
page{
    /* background: linear-gradient(#ff8181 0%, #ffff81 20%, #81ff81 40%, #81ffff 60%, #8181ff 80%, #ff81ff 100%);  */
    background-color: #fff;
border-bottom:1px solid transparent;

}
.launch{
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 999;
}
.launch image{
  width: 100%;
  height: 100%;
}
.wx_code{
  width: 128px;
  height: 128px;
  transform: translateX(-50%);
  position: relative;
  left: 50%;
  top: 20px;
  /* transition: all linear .5s */
}
.wx_code image:first-child{
  display: block;
}
.wx_code image{
  display: none;
  width: 100%;
  height: 100%;
}
.kf_button{
  background-color: transparent;
  position: fixed;
  bottom: 10vh;
  right: 10vw;
}
.kf_button image{
  width: 48px;
  height: 48px;
}
.change{
  padding: 15px;
  margin: 20px auto 15px;
  position: relative;
}
.change .item{
  width: 46%;
  height: 88px;
  margin: 2%;
  text-align: center;
  box-sizing: border-box;
  border: 1px solid #fff;
  box-shadow: 0 0 6px #fff;
  float: left;
  /* display: flex;
  align-items: center;
  justify-content: center; */
  color: #fff;
  font-size: 18px;
  font-weight: 600;
  border-radius: 14px;
  background-repeat: no-repeat;
  background-size: 100%;
  line-height:144px;
}

